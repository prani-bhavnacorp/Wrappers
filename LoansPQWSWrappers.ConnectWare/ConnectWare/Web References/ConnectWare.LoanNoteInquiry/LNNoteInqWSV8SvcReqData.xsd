            <xs:documentation>LN Account Number</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="E300344" type="xs:string">
          <xs:annotation>
            <xs:documentation>External System Account Type</xs:documentation>
          </xs:annotation>
        </xs:element>
