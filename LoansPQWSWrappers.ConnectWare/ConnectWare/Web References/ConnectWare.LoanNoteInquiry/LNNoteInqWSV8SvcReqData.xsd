<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="mtvnCWLNNoteInqReqData" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="mtvnCWLNNoteInqReqData" xmlns:xs="http://www.w3.org/2001/XMLSchema">
