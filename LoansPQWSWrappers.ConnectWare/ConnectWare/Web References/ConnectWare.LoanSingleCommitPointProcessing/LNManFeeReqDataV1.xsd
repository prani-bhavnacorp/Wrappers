          <xs:annotation>
            <xs:documentation>LN Source</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="E31702" type="xs:string">
          <xs:annotation>
            <xs:documentation>LN Batch Number</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="E31703" type="xs:string">
          <xs:annotation>
            <xs:documentation>LN Fee Plan Description</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="E31704" type="xs:string">
          <xs:annotation>
            <xs:documentation>LN Fee Due Date</xs:documentation>
