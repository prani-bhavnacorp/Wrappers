<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="mtvnCWLNFeeWavReqData" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="mtvnCWLNFeeWavReqData" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="LNFeeWavReqData">
    <xs:annotation>
      <xs:documentation>Loan Fee Waiver (LNFWV) -  This function is used to waive all or a portion of an assessed or billed fee for a particular note, commitment, or account. Only billed fees, or the unearned portion of a fee earned over time, may be waived at the note level.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="E30051" type="xs:string">
          <xs:annotation>
            <xs:documentation>Loan Account Number</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="E30053" type="xs:string">
          <xs:annotation>
            <xs:documentation>Loan Account Name</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="E30054" type="xs:string">
          <xs:annotation>
            <xs:documentation>Loan Note Number</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="E33206" type="xs:string">
          <xs:annotation>
            <xs:documentation>Loan Fee Waiver Batch Number</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="E33207" type="xs:string">
          <xs:annotation>
            <xs:documentation>Loan Fee Waiver Transaction Code</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="E33208" type="xs:string">
          <xs:annotation>
            <xs:documentation>Loan Fee Waiver Fee Plan</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="E33209" type="xs:string">
          <xs:annotation>
            <xs:documentation>Loan Fee Waiver Amount</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>