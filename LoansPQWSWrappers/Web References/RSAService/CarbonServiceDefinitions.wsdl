          <ns:documentation>Format of the birthdate object</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element name="year" type="types:year-type" />
          <ns:element name="month" type="types:month-type" />
          <ns:element name="day" type="types:day-type" />
        </ns:sequence>
      </ns:complexType>
