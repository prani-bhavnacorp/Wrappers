        <ns:restriction base="ns:integer">
          <ns:minInclusive value="1" />
          <ns:maxInclusive value="31" />
        </ns:restriction>
      </ns:simpleType>
      <ns:complexType name="answer-type">
        <ns:annotation>
          <ns:documentation>This is the response to a given question. It includes the id of the question as well as the choices selected by the end-user</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element name="question-id" type="ns:long" />
          <ns:element minOccurs="0" name="choices" type="types:choices-type" />
        </ns:sequence>
      </ns:complexType>
      <ns:simpleType name="answer-selection-type">
        <ns:annotation>
          <ns:documentation>Is the answer type of a question single answer or multiple answer</ns:documentation>
        </ns:annotation>
        <ns:restriction base="ns:string">
          <ns:enumeration value="single" />
          <ns:enumeration value="multiple" />
        </ns:restriction>
      </ns:simpleType>
      <ns:complexType name="choice-type">
        <ns:annotation>
          <ns:documentation>Coming from the Verid server this is one of the choices for a given question. It includes the id of the choice and the text to present to the end-user</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element name="choice-id" type="ns:long" />
          <ns:element minOccurs="0" maxOccurs="unbounded" name="text" type="types:text-type" />
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="choices-type">
        <ns:annotation>
          <ns:documentation>An array of choices</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element minOccurs="0" maxOccurs="unbounded" name="choice-id" type="ns:long" />
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="answers-type">
        <ns:annotation>
          <ns:documentation>Being sent to the Verid server as part of a transaction-continue, this contains the answers provided by the end-user in response to a set of questions</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element name="question-set-id" type="ns:long">
