          </ns:element>
          <ns:element minOccurs="0" maxOccurs="unbounded" name="credential-method" type="types:credential-method-type" />
          <ns:element minOccurs="0" name="caller-id" type="ns:string">
            <ns:annotation>
              <ns:documentation>The text that displayed on caller id for the call into the callcenter</ns:documentation>
            </ns:annotation>
          </ns:element>
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="online-type">
        <ns:annotation>
          <ns:documentation>Data collected when the venue is the online</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element minOccurs="0" name="credential" type="types:credential-type" />
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="batch-type">
        <ns:annotation>
          <ns:documentation>Data collected when the venue is of type batch</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element minOccurs="0" name="batch-id" type="types:batch-id-type" />
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="netview-type">
        <ns:annotation>
          <ns:documentation>Data collected when the venue is of type netview</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element minOccurs="0" name="netview-form" type="types:netview-form-type" />
        </ns:sequence>
      </ns:complexType>
