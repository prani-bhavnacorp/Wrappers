<?xml version="1.0" encoding="utf-8"?>
<s0:definitions xmlns:s1="java:com.verid.carbon.integration.datatypes" xmlns:s3="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:s2="http://integration.carbon.verid.com" xmlns="" name="CarbonServiceDefinitions" targetNamespace="http://integration.carbon.verid.com" xmlns:s0="http://schemas.xmlsoap.org/wsdl/">
  <s0:types>
    <xs:schema xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:stns="java:com.verid.carbon.integration.datatypes" xmlns:wsd="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="http://integration.carbon.verid.com" xmlns:s1="java:com.verid.carbon.integration.datatypes" xmlns="java:com.verid.carbon.integration.datatypes" xmlns:ns="http://www.w3.org/2001/XMLSchema" xmlns:s3="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:types="java:com.verid.carbon.integration.datatypes" xmlns:s2="http://integration.carbon.verid.com" xmlns:ns1="http://schemas.xmlsoap.org/soap/encoding/" attributeFormDefault="unqualified" elementFormDefault="unqualified" targetNamespace="java:com.verid.carbon.integration.datatypes" jaxb:version="2.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
      <ns:simpleType name="account-name-type">
        <ns:restriction base="ns:string" />
      </ns:simpleType>
      <ns:simpleType name="transaction-direction-type">
        <ns:restriction base="ns:string">
          <ns:enumeration value="buy" />
          <ns:enumeration value="sell" />
        </ns:restriction>
      </ns:simpleType>
      <ns:simpleType name="product-category-type">
        <ns:restriction base="ns:string">
          <ns:enumeration value="accounting" />
          <ns:enumeration value="alcohol" />
          <ns:enumeration value="amusement-and-recreation-services" />
          <ns:enumeration value="apparel" />
          <ns:enumeration value="audio" />
          <ns:enumeration value="auto-rental-and-Leasing" />
          <ns:enumeration value="automotive-service" />
          <ns:enumeration value="baby" />
          <ns:enumeration value="background-check" />
          <ns:enumeration value="bond" />
          <ns:enumeration value="book" />
          <ns:enumeration value="cable-or-pay-television-services" />
          <ns:enumeration value="caller-id-spoofing" />
          <ns:enumeration value="camera" />
          <ns:enumeration value="certificate-of-deposit" />
          <ns:enumeration value="computer" />
          <ns:enumeration value="concert" />
          <ns:enumeration value="construction-material" />
          <ns:enumeration value="cosmetic" />
          <ns:enumeration value="credit-check" />
          <ns:enumeration value="electronics" />
          <ns:enumeration value="equipment-rental" />
          <ns:enumeration value="footwear" />
          <ns:enumeration value="furniture-or-home-furnishings" />
          <ns:enumeration value="future" />
          <ns:enumeration value="game-or-toy" />
          <ns:enumeration value="gas" />
          <ns:enumeration value="grocery" />
          <ns:enumeration value="hotel" />
          <ns:enumeration value="insurance" />
          <ns:enumeration value="investment-advice" />
          <ns:enumeration value="jewelry" />
          <ns:enumeration value="kitchen-and-housewares" />
          <ns:enumeration value="lawn-and-garden-equipment" />
          <ns:enumeration value="membership-organizations" />
          <ns:enumeration value="money-transfer-in" />
          <ns:enumeration value="money-transfer-out" />
          <ns:enumeration value="motor-vehicle" />
          <ns:enumeration value="music" />
          <ns:enumeration value="musical-instrument" />
          <ns:enumeration value="office-product" />
          <ns:enumeration value="option" />
          <ns:enumeration value="pet-supplies" />
          <ns:enumeration value="real-estate" />
          <ns:enumeration value="restaurant" />
          <ns:enumeration value="software" />
          <ns:enumeration value="sporting-and-athletic-product" />
          <ns:enumeration value="sporting-event" />
          <ns:enumeration value="sports-and-recreation-club" />
          <ns:enumeration value="stock" />
          <ns:enumeration value="subscriptions-on-line-content" />
          <ns:enumeration value="subscriptions-other" />
          <ns:enumeration value="subscriptions-periodical" />
          <ns:enumeration value="tobacco" />
          <ns:enumeration value="tools-and-hardware" />
          <ns:enumeration value="trucking-or-courier-service" />
          <ns:enumeration value="video-purchase" />
          <ns:enumeration value="video-rental" />
          <ns:enumeration value="wager" />
          <ns:enumeration value="other" />
          <ns:enumeration value="deposit" />
          <ns:enumeration value="withdrawal" />
          <ns:enumeration value="bill-payment" />
        </ns:restriction>
      </ns:simpleType>
      <ns:complexType name="product-type">
        <ns:sequence>
          <ns:element name="product-category" type="types:product-category-type" />
          <ns:element minOccurs="0" name="model" type="ns:string" />
          <ns:element minOccurs="0" name="vendor" type="ns:string" />
          <ns:element minOccurs="0" name="code" type="ns:string" />
          <ns:element minOccurs="0" name="symbol" type="ns:string" />
          <ns:element minOccurs="0" name="transaction-direction" type="types:transaction-direction-type" />
          <ns:element minOccurs="0" name="product-quantity" type="ns:integer" />
          <ns:element minOccurs="0" name="per-item-amount" type="ns:decimal" />
          <ns:element minOccurs="0" name="transaction-date" type="types:transaction-date-type" />
          <ns:element minOccurs="0" name="data" type="types:complex-detail-type" />
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="person-type">
        <ns:annotation>
          <ns:documentation>This construct captures information about the person being authenticated</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element minOccurs="0" name="name-prefix" type="ns:string">
            <ns:annotation>
              <ns:documentation>Person's name's prefix if any (e.g., Mr., Mrs., Dr., ...)</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="name-first" type="ns:string">
            <ns:annotation>
              <ns:documentation>Person's given name</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="name-middle" type="ns:string">
            <ns:annotation>
              <ns:documentation>Person's middle name</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="name-last" type="ns:string">
            <ns:annotation>
              <ns:documentation>Person's surname</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="name-suffix" type="ns:string">
            <ns:annotation>
              <ns:documentation>Person's name's suffix (e.g., Jr, III, ...)</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" maxOccurs="unbounded" name="customer-id" type="types:customer-id-type" />
          <ns:element minOccurs="0" maxOccurs="unbounded" name="email" type="types:email-type" />
          <ns:element minOccurs="0" name="ssn" type="ns:string">
            <ns:annotation>
              <ns:documentation>Person's social security number (if provided)</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="ssn-type" type="types:ssn-type">
            <ns:annotation>
              <ns:documentation>Type of the social security number provided (none, last4 or full)</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="drivers-license-number" type="ns:string">
            <ns:annotation>
              <ns:documentation>Person's drivers license number (if provided)</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="drivers-license-state" type="ns:string">
            <ns:annotation>
              <ns:documentation>State where drivers license issued (if provided, must be a valid 2 letter USPS state code)</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="birthdate" type="types:birthdate-type">
            <ns:annotation>
              <ns:documentation>Person's birthdate</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" maxOccurs="unbounded" name="address" type="types:address-type">
            <ns:annotation>
              <ns:documentation>Addresses associated with the person</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" maxOccurs="unbounded" name="uk-address" type="types:uk-address-type">
            <ns:annotation>
              <ns:documentation>UK Addresses associated with the person</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" maxOccurs="unbounded" name="phone-number" type="types:phone-number-type">
            <ns:annotation>
              <ns:documentation>Phone numbers associated with the person</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="business" type="types:business-type" />
          <ns:element minOccurs="0" maxOccurs="unbounded" name="employer" type="types:business-type" />
          <ns:element minOccurs="0" name="occupation" type="ns:string" />
          <ns:element minOccurs="0" name="risk-assessment" type="types:risk-assessment-type" />
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="address-type">
        <ns:annotation>
          <ns:documentation>Multi-purpose address container</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element name="address-street1" type="ns:string">
            <ns:annotation>
              <ns:documentation>Part 1 of the street name</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="address-street2" type="ns:string">
            <ns:annotation>
              <ns:documentation>Part 2 of the street name</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="suite" type="ns:string">
            <ns:annotation>
              <ns:documentation>Suite or apartment number</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="address-city" type="ns:string">
            <ns:annotation>
              <ns:documentation>City of the address</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="address-state" type="ns:string">
            <ns:annotation>
              <ns:documentation>State of the address (must be a valid 2 letter USPS state code)</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="address-zip" type="ns:string">
            <ns:annotation>
              <ns:documentation>Zip of the address</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="address-zip-plus4" type="ns:string">
            <ns:annotation>
              <ns:documentation>Zip4 of the address</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="address-country" type="ns:string">
            <ns:annotation>
              <ns:documentation>Not required: Country of the address (assumed to be US)</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="address-context" type="types:address-context-type">
            <ns:annotation>
              <ns:documentation>The relationship between the address and the owning construct</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="address-years-at" type="ns:decimal">
            <ns:annotation>
              <ns:documentation>Years of residence at this address (1.3, 0.2, etc.)</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="occupation-start-date" type="ns:date" />
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="uk-address-type">
        <ns:annotation>
          <ns:documentation>Multi-purpose address container</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element name="house-name" type="ns:string">
            <ns:annotation>
              <ns:documentation>House name</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="house-number" type="ns:string">
            <ns:annotation>
              <ns:documentation>House number</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="street1" type="ns:string">
            <ns:annotation>
              <ns:documentation>Street1 name</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="street2" type="ns:string">
            <ns:annotation>
              <ns:documentation>Street2 name</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="district" type="ns:string">
            <ns:annotation>
              <ns:documentation>District name</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="town" type="ns:string">
            <ns:annotation>
              <ns:documentation>Town of the address</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="county" type="ns:string">
            <ns:annotation>
              <ns:documentation>County of the address</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="postcode" type="ns:string">
            <ns:annotation>
              <ns:documentation>Postcode of the address</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="context" type="types:address-context-type">
            <ns:annotation>
              <ns:documentation>The relationship between the address and the owning construct</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="years-at" type="ns:decimal">
            <ns:annotation>
              <ns:documentation>Years of residence at this address (1.3, 0.2, etc.)</ns:documentation>
            </ns:annotation>
          </ns:element>
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="phone-number-type">
        <ns:annotation>
          <ns:documentation>Representation of a phone number</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element name="phone-number" type="ns:string">
            <ns:annotation>
              <ns:documentation>The phone number</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="phone-number-context" type="types:phone-number-context-type">
            <ns:annotation>
              <ns:documentation>The relationship between the phone number and the owning construct</ns:documentation>
            </ns:annotation>
          </ns:element>
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="business-type">
        <ns:annotation>
          <ns:documentation>Representation of a business</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element name="company-name" type="ns:string">
            <ns:annotation>
              <ns:documentation>The name of the business</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="fein" type="ns:string">
            <ns:annotation>
              <ns:documentation>The business's FEIN</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="address" type="types:address-type">
            <ns:annotation>
              <ns:documentation>Array of addresses associated with the business (typically only one is given)</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" maxOccurs="unbounded" name="phone-number" type="types:phone-number-type">
            <ns:annotation>
              <ns:documentation>Array of phone numbers associated with the business (typically only one is given)</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="title" type="ns:string" />
          <ns:element minOccurs="0" name="start-date" type="ns:date" />
          <ns:element minOccurs="0" name="end-date" type="ns:date" />
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="ordered-by-type">
        <ns:annotation>
          <ns:documentation>Who is placing the "order".</ns:documentation>
        </ns:annotation>
        <ns:choice>
          <ns:element name="same-as" type="types:same-as-type" />
          <ns:element name="person" type="types:person-type" />
        </ns:choice>
      </ns:complexType>
      <ns:complexType name="bill-to-type">
        <ns:annotation>
          <ns:documentation>Optional: To whom is the "Order" being billed. Populate either the contact or the same-as field but not both.</ns:documentation>
        </ns:annotation>
        <ns:choice>
          <ns:element name="same-as" type="types:same-as-type" />
          <ns:element name="person-type" type="types:person-type" />
        </ns:choice>
      </ns:complexType>
      <ns:complexType name="ship-to-type">
        <ns:annotation>
          <ns:documentation>Optional: To whom is the "Order" being shipped. Populate either the contact or the same-as field but not both.</ns:documentation>
        </ns:annotation>
        <ns:choice minOccurs="0">
          <ns:element name="same-as" type="types:same-as-type" />
          <ns:element name="person-type" type="types:person-type" />
        </ns:choice>
      </ns:complexType>
      <ns:simpleType name="same-as-type">
        <ns:annotation>
          <ns:documentation>Available values for the same-as field.  Note that the bill-to value is only valid when used in the context of the ship-to-type.</ns:documentation>
        </ns:annotation>
        <ns:restriction base="ns:string">
          <ns:enumeration value="ordered-by">
            <ns:annotation>
              <ns:documentation>Use the ordered-by contact.</ns:documentation>
            </ns:annotation>
          </ns:enumeration>
          <ns:enumeration value="bill-to">
            <ns:annotation>
              <ns:documentation>Use the bill-to contact.</ns:documentation>
            </ns:annotation>
          </ns:enumeration>
        </ns:restriction>
      </ns:simpleType>
      <ns:simpleType name="payment-method">
        <ns:restriction base="ns:string">
          <ns:enumeration value="store-credit" />
          <ns:enumeration value="gift-card" />
          <ns:enumeration value="money-transfer" />
          <ns:enumeration value="cash" />
          <ns:enumeration value="check" />
          <ns:enumeration value="credit-card" />
          <ns:enumeration value="debit-card" />
          <ns:enumeration value="phone" />
          <ns:enumeration value="bill-general" />
          <ns:enumeration value="bill-paper" />
          <ns:enumeration value="margin" />
          <ns:enumeration value="other" />
        </ns:restriction>
      </ns:simpleType>
      <ns:complexType name="purchase-type">
        <ns:annotation>
          <ns:documentation>Details of the purchase transaction type.</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element minOccurs="0" name="order">
            <ns:complexType>
              <ns:sequence>
                <ns:element maxOccurs="unbounded" name="item" type="types:product-type" />
                <ns:element name="total-amount" type="ns:decimal" />
                <ns:element minOccurs="0" name="total-tax" type="ns:decimal" />
                <ns:element minOccurs="0" name="total-shipping" type="ns:decimal" />
                <ns:element minOccurs="0" name="merchant" type="ns:string" />
              </ns:sequence>
            </ns:complexType>
          </ns:element>
          <ns:element minOccurs="0" name="payment-method" type="types:payment-method" />
          <ns:element minOccurs="0" name="shipping" type="types:shipping-type" />
          <ns:element name="account" type="types:account-type" />
          <ns:element name="venue" type="types:venue-type" />
          <ns:element name="transaction-date" type="types:transaction-date-type" />
          <ns:element minOccurs="0" name="risk-assessment" type="types:risk-assessment-type" />
        </ns:sequence>
      </ns:complexType>
      <ns:simpleType name="mode-type">
        <ns:annotation>
          <ns:documentation>Mode of the transaction. Typically set to "live"</ns:documentation>
        </ns:annotation>
        <ns:restriction base="ns:string">
          <ns:enumeration value="simulated" />
          <ns:enumeration value="testing" />
          <ns:enumeration value="pilot" />
          <ns:enumeration value="live" />
          <ns:enumeration value="verid" />
          <ns:enumeration value="verid_cs" />
        </ns:restriction>
      </ns:simpleType>
      <ns:simpleType name="shipping-method">
        <ns:restriction base="ns:string">
          <ns:enumeration value="ground" />
          <ns:enumeration value="priority-overnight" />
          <ns:enumeration value="next-day-air" />
          <ns:enumeration value="in-store-pickup" />
          <ns:enumeration value="second-day-air" />
          <ns:enumeration value="priority-ground" />
        </ns:restriction>
      </ns:simpleType>
      <ns:complexType name="shipping-type">
        <ns:sequence>
          <ns:element name="bill-to" type="types:bill-to-type" />
          <ns:element name="ship-to" type="types:ship-to-type" />
          <ns:element minOccurs="0" name="shipping-method" type="types:shipping-method" />
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="settings-type">
        <ns:annotation>
          <ns:documentation>Any overrides to the controls specified in the transaction's ruleset that are to be used for this particular transaction</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element name="account-name" type="types:account-name-type">
            <ns:annotation>
              <ns:documentation>The account name to which this transaction will be associated.</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="mode" type="types:mode-type">
            <ns:annotation>
              <ns:documentation>The mode that the transaction will run under</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="ruleset" type="ns:string">
            <ns:annotation>
              <ns:documentation>Ruleset that will be used to govern the operation of the transaction.  See your account representative for possible values</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="simulator-mode" type="types:simulator-mode-type">
            <ns:annotation>
              <ns:documentation>If the mode of the transaction is "simulated" then this field governs the response given by the Verid system</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" name="attachment-type" type="types:attachment-type" />
          <ns:element minOccurs="0" maxOccurs="unbounded" name="special-feature" type="types:special-feature-type" />
          <ns:element minOccurs="0" maxOccurs="unbounded" name="internationalization" type="types:internationalization-type" />
          <ns:element minOccurs="0" maxOccurs="unbounded" name="reference-id">
            <ns:simpleType>
              <ns:restriction base="types:reference-id-type" />
            </ns:simpleType>
          </ns:element>
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="disposition-settings-type">
        <ns:complexContent mixed="false">
          <ns:extension base="types:settings-type">
            <ns:sequence>
              <ns:element name="transaction-id" type="types:transaction-id-type" />
            </ns:sequence>
          </ns:extension>
        </ns:complexContent>
      </ns:complexType>
      <ns:complexType name="continue-settings-type">
        <ns:complexContent mixed="false">
          <ns:extension base="types:settings-type">
            <ns:sequence>
              <ns:element name="transaction-id" type="types:transaction-id-type" />
            </ns:sequence>
          </ns:extension>
        </ns:complexContent>
      </ns:complexType>
      <ns:complexType name="resume-settings-type">
        <ns:complexContent mixed="false">
          <ns:extension base="types:settings-type">
            <ns:sequence>
              <ns:element name="transaction-id" type="types:transaction-id-type" />
            </ns:sequence>
          </ns:extension>
        </ns:complexContent>
      </ns:complexType>
      <ns:complexType name="identity-verification-settings-type">
        <ns:complexContent mixed="false">
          <ns:extension base="types:settings-type">
            <ns:sequence>
              <ns:element name="task" type="types:task-type" />
              <ns:element minOccurs="0" name="sequence-id" type="ns:integer" />
              <ns:element minOccurs="0" name="agent" type="ns:string" />
              <ns:element minOccurs="0" name="patriot-act-compliance" type="ns:boolean" />
              <ns:element minOccurs="0" name="parent-transaction-id" type="types:transaction-id-type" />
            </ns:sequence>
          </ns:extension>
        </ns:complexContent>
      </ns:complexType>
      <ns:complexType name="verification-type">
        <ns:annotation>
          <ns:documentation>The verification transaction element.</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element name="venue" type="types:venue-type" />
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="venue-type">
        <ns:annotation>
          <ns:documentation>Environment of the transaction</ns:documentation>
        </ns:annotation>
        <ns:choice>
          <ns:element name="online" type="types:online-type">
            <ns:annotation>
              <ns:documentation>The Verid system is being called as part of an integrated on-line solution.</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="callcenter" type="types:callcenter-type">
            <ns:annotation>
              <ns:documentation>The Verid system is being called as part of an integrated call-center solution.</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="batch" type="types:batch-type">
            <ns:annotation>
              <ns:documentation>The Verid system is being called as part of a batch submission</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="netview" type="types:netview-type">
            <ns:annotation>
              <ns:documentation>The Verid system is being called by Verid's GUI</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="point-of-sale">
            <ns:complexType>
              <ns:complexContent mixed="false">
                <ns:extension base="types:point-of-sale-type">
                  <ns:sequence>
                    <ns:element minOccurs="0" maxOccurs="unbounded" name="credential-method" type="types:credential-method-type" />
                    <ns:element minOccurs="0" name="merchant-zip-code" type="ns:string" />
                  </ns:sequence>
                </ns:extension>
              </ns:complexContent>
            </ns:complexType>
          </ns:element>
          <ns:element name="customer-service" type="types:customer-service-type" />
          <ns:element name="ivr" type="types:ivr-type" />
        </ns:choice>
      </ns:complexType>
      <ns:complexType name="callcenter-type">
        <ns:annotation>
          <ns:documentation>Data collected when the venue is the callcenter</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element minOccurs="0" name="caller-phone-number" type="ns:string">
            <ns:annotation>
              <ns:documentation>The phone number that the caller used to call into the callcenter</ns:documentation>
