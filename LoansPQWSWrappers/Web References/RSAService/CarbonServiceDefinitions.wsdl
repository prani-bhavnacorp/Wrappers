      <ns:complexType name="simple-detail-type">
        <ns:annotation>
          <ns:documentation>Textual descriptive information</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element name="text" type="ns:string">
            <ns:annotation>
              <ns:documentation>Text for the simple detail</ns:documentation>
            </ns:annotation>
          </ns:element>
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="complex-detail-type">
        <ns:annotation>
          <ns:documentation>Textual information is passed back in a bullet-type hierarchy of points. A complex-detail can contain a list of 0 or more of either complex and simple details (note that a complex-detail cannot contain both simple and complex detail arrays)</ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element name="heading" type="ns:string">
            <ns:annotation>
              <ns:documentation>Heading text for the complex detail</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" maxOccurs="unbounded" name="simple-detail" type="types:simple-detail-type" />
          <ns:element minOccurs="0" maxOccurs="unbounded" name="complex-detail" type="types:complex-detail-type" />
        </ns:sequence>
      </ns:complexType>
      <ns:complexType name="information-type">
        <ns:annotation>
          <ns:documentation>Informational fact that can contain either an array of complex-details or an array of simple-details </ns:documentation>
        </ns:annotation>
        <ns:sequence>
          <ns:element name="information-code" type="types:information-code-type" />
          <ns:element name="detail-code" type="ns:string">
            <ns:annotation>
              <ns:documentation>Actionable string representing the explicit information element</ns:documentation>
            </ns:annotation>
          </ns:element>
          <ns:element name="detail-description" type="ns:string" />
          <ns:element minOccurs="0" maxOccurs="unbounded" name="complex-detail" type="types:complex-detail-type" />
          <ns:element minOccurs="0" maxOccurs="unbounded" name="simple-detail" type="types:simple-detail-type" />
        </ns:sequence>
