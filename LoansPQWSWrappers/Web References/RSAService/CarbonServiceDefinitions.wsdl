            </ns:annotation>
          </ns:element>
          <ns:element minOccurs="0" maxOccurs="unbounded" name="answer" type="types:answer-type">
            <ns:annotation>
              <ns:documentation>Answers to each of the questions provided by the end-user</ns:documentation>
            </ns:annotation>
          </ns:element>
        </ns:sequence>
      </ns:complexType>
