    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="Release2008R2">
                    <security mode="Transport" />
                </binding>
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="https://idws-vegas.syndication.kbb.com/3.0/VehicleInformationService2008R2.svc/Soap11"
                binding="basicHttpBinding" bindingConfiguration="Release2008R2"
                contract="KBB3.InfoDriver.IVehicleInformationService2008R2"
