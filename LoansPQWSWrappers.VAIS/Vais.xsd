			<xs:sequence>
				<xs:element ref="TaxId" minOccurs="0"/>
				<xs:element name="Name" type="Name_Type">
					<xs:annotation>
						<xs:documentation>Appliicant's name.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Address" type="Address_Type" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Applicant's address.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PhoneNumber" type="PhoneNumber_Type" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Applicant's phone number including area code.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MarketSegment" type="MarketSegment_Type" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Market segment option.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Comment" type="Comment_Type" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Free form comment area.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BankingSegment" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AccountPerformanceData" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Request only -- provided by issuing members.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DateOfBirth" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Date of birth</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="8"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DriversLicenseInfo" type="DriversLicense_Type" minOccurs="0"/>
				<xs:element name="SecondPhoneNumber" type="PhoneNumber_Type" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Applicant's secondary phone number including area code.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DepositAccountNumber" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="16"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ProductCode" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="AcquisitionChannel" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
