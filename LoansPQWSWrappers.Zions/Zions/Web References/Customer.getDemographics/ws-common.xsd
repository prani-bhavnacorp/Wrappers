      <xsd:documentation>
                C, D, or M for DDA.
                M for Savings.
                Does limited checking with acct type and application.  (The program will
                accept a P or C for DDA or Savings but Zeus won't send it.)
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="C" />
      <xsd:enumeration value="D" />
      <xsd:enumeration value="M" />
      <xsd:enumeration value="P" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="StatementTermType">
    <xsd:annotation>
      <xsd:documentation>
                If statement frequency is D or M, this is the statement term.
                If C it is the cycle number.
                If P it is the first statement day of the month.
                No other cutoff days are currently included here for P.  P.3-42.
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="0" />
      <xsd:maxInclusive value="999" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="StatusCodeType">
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0" />
      <xsd:maxLength value="1" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="StatusType">
    <xsd:restriction base="common:OneCharacterCodeType" />
  </xsd:simpleType>
  <xsd:simpleType name="StreetAddressType">
    <xsd:annotation>
      <xsd:documentation>
                Street Address
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1" />
      <xsd:maxLength value="40" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="String40Type">
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0" />
      <xsd:maxLength value="40" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ThreeCharacterCodeType">
    <xsd:annotation>
      <xsd:documentation>
                Used for three character codes where the 
                enumeration is not known
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0" />
      <xsd:maxLength value="3" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="TiebreakerNumberType">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="1" />
      <xsd:maxInclusive value="9999" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="TinType">
    <xsd:annotation>
      <xsd:documentation>
                Can contain a TIN or SSN with or without hyphens
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="(\d{3}-\d{2}-\d{4})|(\d{9})|(\d{2}-\d{7})?" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="TinTypeType">
    <xsd:annotation>
      <xsd:documentation>
              F - US federal ID (businesses)             
              S - US social security Number (individuals)
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="F" />
      <xsd:enumeration value="S" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="TraceLevelType">
    <xsd:annotation>
      <xsd:documentation>
				Indicates level at with to trace to the log
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ALL" />
      <xsd:enumeration value="TRACE" />
      <xsd:enumeration value="DEBUG" />
      <xsd:enumeration value="INFO" />
      <xsd:enumeration value="WARN" />
      <xsd:enumeration value="ERROR" />
      <xsd:enumeration value="FATAL" />
      <xsd:enumeration value="OFF" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="TransactionCodeType">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="1" />
      <xsd:maxInclusive value="9999999" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="TwoCharacterCodeType">
    <xsd:annotation>
      <xsd:documentation>
                Used for two character codes where the 
                enumeration is not known
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:length value="2" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="UserIdType">
    <xsd:annotation>
      <xsd:documentation>Identifies the user that is making the inquiry.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1" />
      <xsd:maxLength value="40" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="YesNoIndicator">
    <xsd:annotation>
      <xsd:documentation>
				Boolean indicator Y/N
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Y" />
      <xsd:enumeration value="N" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="YearType">
    <xsd:restriction base="xsd:int">
      <xsd:pattern value="\d{4}" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ZipCodeType">
    <xsd:annotation>
      <xsd:documentation>Zip code portion of address</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\d{5}(-\d{4})?" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="AccountNumberListType">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" ref="common:AccountNumber" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CommonServiceRequestType">
    <xsd:sequence>
      <xsd:element ref="common:Hash" />
      <xsd:element ref="common:TransactionId" />
      <xsd:element ref="common:Channel" />
      <xsd:element ref="common:UserId" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CommonServiceResponseType">
    <xsd:sequence>
      <xsd:element ref="common:Code" />
      <xsd:element ref="common:Message" />
      <xsd:element ref="common:TransactionId" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CustomerInformationType">
    <xsd:sequence>
      <xsd:element minOccurs="0" ref="common:CustomerNumber" />
      <xsd:element minOccurs="0" ref="common:BankNumber" />
      <xsd:element minOccurs="0" ref="common:CustomerName" />
      <xsd:element minOccurs="0" ref="common:CustomerTypeCode" />
      <xsd:element minOccurs="0" ref="common:Alpha" />
      <xsd:element minOccurs="0" ref="common:Accum" />
      <xsd:element minOccurs="0" ref="common:TiebreakerNumber" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="EmailType">
    <xsd:annotation>
      <xsd:documentation>Represents an email address</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element ref="common:EmailAddress" />
      <xsd:element minOccurs="0" ref="common:EmailTypeCode" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="IdType">
    <xsd:sequence>
      <xsd:element ref="common:IdType" />
      <xsd:element ref="common:IssuerCode" />
      <xsd:element ref="common:IdNumber" />
      <xsd:element ref="common:IssueDate" />
      <xsd:element ref="common:ExpirationDate" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="MessageType">
    <xsd:sequence>
      <xsd:element minOccurs="0" ref="common:MessageIndicator" />
      <xsd:element minOccurs="0" ref="common:Message" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SignerType">
    <xsd:sequence>
      <xsd:element ref="common:CustomerName" />
      <xsd:element ref="common:TiebreakerNumber" />
      <xsd:choice>
        <xsd:element ref="common:RelationshipCode" />
        <xsd:element ref="common:RelationshipType" />
      </xsd:choice>
      <xsd:element minOccurs="0" ref="common:CommercialRetailCode" />
      <xsd:element minOccurs="0" ref="common:FullAddress" />
      <xsd:element minOccurs="0" ref="common:CipEditCode" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SignerListType">
    <xsd:sequence>
      <xsd:element maxOccurs="9" ref="common:Signer" />
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>