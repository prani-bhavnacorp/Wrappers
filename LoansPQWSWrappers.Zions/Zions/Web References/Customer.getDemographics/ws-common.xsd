  <xsd:simpleType name="AccountNumberType">
    <xsd:annotation>
      <xsd:documentation>
                Account Number to which this information applies.
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1" />
      <xsd:maxLength value="20" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AggregateInterestRateType">
    <xsd:annotation>
      <xsd:documentation>
                Code for type of interest rate
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:decimal">
      <xsd:fractionDigits value="5" />
      <xsd:totalDigits value="11" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AlternateContactTypeType">
    <xsd:annotation>
      <xsd:documentation>
				Indicates an alternate contact type
				C = Cell Phone
				P = Pager
				O = Other
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="C" />
      <xsd:enumeration value="P" />
      <xsd:enumeration value="O" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AccountStatusCodeType">
    <xsd:annotation>
      <xsd:documentation>
                Account Status Code for a given Account Number.
            Space = Normal Processing or Status Unavailable    
                A = Auth Prohibited
                B = Bankrupt
                C = Closed
                D = Dormant
                E = Revoked
                F = Frozen
                I = Inactive
                L = Lost
                N = New Today
                O = Open
                P = Pending Purge
                R = Recent Open
                U = Stolen
                0 = Open
                1 = Blocked
                2 = Warm
                3 = Hot (Lost/Stolen)
                4 = Closed
                5 = Blocked
                8 = Closed This Month
                9 = Closed Last Month
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value=" " />
      <xsd:enumeration value="A" />
      <xsd:enumeration value="B" />
      <xsd:enumeration value="C" />
      <xsd:enumeration value="D" />
      <xsd:enumeration value="E" />
      <xsd:enumeration value="F" />
      <xsd:enumeration value="I" />
      <xsd:enumeration value="L" />
      <xsd:enumeration value="N" />
      <xsd:enumeration value="O" />
      <xsd:enumeration value="P" />
      <xsd:enumeration value="R" />
      <xsd:enumeration value="U" />
      <xsd:enumeration value="0" />
      <xsd:enumeration value="1" />
      <xsd:enumeration value="2" />
      <xsd:enumeration value="3" />
      <xsd:enumeration value="4" />
      <xsd:enumeration value="5" />
      <xsd:enumeration value="8" />
      <xsd:enumeration value="9" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AccountStatusType">
    <xsd:annotation>
      <xsd:documentation>
                Account Status for a given Account Number.
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="CLOSED" />
      <xsd:enumeration value="DELETED" />
      <xsd:enumeration value="ERASED" />
      <xsd:enumeration value="OPEN" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AccountTypeCodeType">
    <xsd:annotation>
      <xsd:documentation>Account Code</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="3" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AddUpdateDeleteCodeType">
    <xsd:annotation>
      <xsd:documentation>
				Indicates Add Update or Delete
				A = Add
				D = Delete
				U = Update
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="A" />
      <xsd:enumeration value="D" />
      <xsd:enumeration value="U" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="BankCodeType">
    <xsd:annotation>
      <xsd:documentation>
                Bank Code
                Blank for Zions
                N: Nevada
                C: Colorado
                etc...
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="common:OneCharacterCodeType" />
  </xsd:simpleType>
  <xsd:simpleType name="BankNumberType">
    <xsd:annotation>
      <xsd:documentation>
                Identification number of the affiliate.
                0001: Zions First National Bank
                0013: Vectrabank Colorado
                0018: Nevada State Bank
                0116: National Bank of Arizona
                0140: California Bank and Trust
                0150: Commerce Bank of Washington
                0175: Amegy Bank Texas
                0190: Commerce Bank of Oregon
            </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="0001" />
      <xsd:enumeration value="0013" />
      <xsd:enumeration value="0018" />
      <xsd:enumeration value="0116" />
      <xsd:enumeration value="0140" />
      <xsd:enumeration value="0150" />
      <xsd:enumeration value="0175" />
      <xsd:enumeration value="0190" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="BranchNumberType">
    <xsd:annotation>
      <xsd:documentation>Customer Branch</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="0" />
      <xsd:maxInclusive value="99999" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CardNumberType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{16}" />
    </xsd:restriction>
  </xsd:simpleType>
