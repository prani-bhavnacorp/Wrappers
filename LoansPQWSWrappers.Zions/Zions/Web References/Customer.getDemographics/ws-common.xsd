      <xsd:documentation>Country code portion of foreign address</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="common:ThreeCharacterCodeType" />
  </xsd:simpleType>
  <xsd:simpleType name="CountType">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="0" />
      <xsd:maxInclusive value="999" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CustomerAccumType">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="0" />
      <xsd:maxInclusive value="999" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CustomerAlphaType">
