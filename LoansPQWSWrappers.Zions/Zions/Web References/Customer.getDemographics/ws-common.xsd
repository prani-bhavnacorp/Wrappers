      <xsd:enumeration value="BORROWER 2" />
      <xsd:enumeration value="CO-MAKER 1" />
      <xsd:enumeration value="CO-MAKER 2" />
      <xsd:enumeration value="AUTH DEBIT" />
      <xsd:enumeration value="ATTORNEY" />
      <xsd:enumeration value="BENEFICIARY" />
      <xsd:enumeration value="CO-BORROWER" />
      <xsd:enumeration value="CO-EXECUTOR" />
      <xsd:enumeration value="CO-TRUSTEE" />
      <xsd:enumeration value="CUSTODIAN" />
      <xsd:enumeration value="EXECUTOR" />
      <xsd:enumeration value="GRANTOR" />
