      <xsd:enumeration value="AmazingRewards" />
      <xsd:enumeration value="Appro" />
      <xsd:enumeration value="BankCard" />
      <xsd:enumeration value="BPM" />
      <xsd:enumeration value="branch" />
      <xsd:enumeration value="CallCenter" />
      <xsd:enumeration value="CapitalStream" />
      <xsd:enumeration value="ContactManagement" />
      <xsd:enumeration value="CorporateBanking" />
      <xsd:enumeration value="esspsoa" />
      <xsd:enumeration value="FileNet" />
      <xsd:enumeration value="Internet" />
      <xsd:enumeration value="internetbanking" />
      <xsd:enumeration value="IVR" />
      <xsd:enumeration value="Mobile" />
      <xsd:enumeration value="NetCaptureBranch" />
      <xsd:enumeration value="OpenSpan" />
      <xsd:enumeration value="Portal" />
      <xsd:enumeration value="RefundTracking" />
      <xsd:enumeration value="RetailLoanOrigination" />
      <xsd:enumeration value="ServiceIntegrator" />
      <xsd:enumeration value="Wires" />
      <xsd:enumeration value="Test" />
      <xsd:enumeration value="zeus" />
